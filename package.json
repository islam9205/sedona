{
  "name": "sedona",
  "private": true,
  "description": "Личный проект «{{projectTitle}}» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.2.5",
    "browser": "0.2.6",
    "browser-sync": "2.26.14",
    "gulp": "4.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-less": "4.0.1",
    "gulp-libsquoosh": "^1.1.2",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-svgstore": "^9.0.0",
    "gulp-webp": "^4.0.1",
    "postcss": "8.2.8",
    "postcss-csso": "^6.0.1",
    "stylelint": "13.13.1",
    "stylelint-config-htmlacademy": "0.1.10"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/less/**/*.less\" --syntax less",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp styles",
    "start": "gulp",
    "html": "gulp html",
    "copyImages": "gulp copyImages",
    "optimizeImages": "gulp optimizeImages",
    "createWebp": "gulp createWebp"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/less/**/*.less"
  ],
  "engines": {
    "node": "^12 || ^14"
  },
  "dependencies": {
    "browser-sync-client": "^2.27.9"
  }
}
